{
  "container": {
    "docker": {
      "image": "redis",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 6379, "hostPort": 6379, "servicePort": 6379, "protocol": "tcp" }
      ]
    }
  },
  "id": "redis",
  "type": "DOCKER",
  "instances": 1,
  "cpus": 0.5,
  "mem": 128,
  "uris": [],
  "constraints": [["hostname", "UNIQUE", "10.0.1.81"]]
}
